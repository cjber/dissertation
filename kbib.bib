  date = {2012-06},
  pages = {9-16},
  author = {Zhao, Jiaping and You, Suya},
  file = {/home/cjber/drive/pdf/ENVS492/zhao2012.pdf;/home/cjber/drive/pdf/ENVS492/zhao22.pdf}
